<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <AnchorPane layoutX="-7.0" layoutY="-6.0" onDragDetected="#goToSignIn" prefHeight="318.0" prefWidth="608.0">
      <children>
      	 <TextField fx:id="username" layoutX="83.0" layoutY="154.0" promptText="Username" />
      	 <TextField fx:id="email" layoutX="253.0" layoutY="154.0" prefHeight="25.0" prefWidth="259.0" promptText="email" />
         <TextField fx:id="password" layoutX="83.0" layoutY="197.0" prefHeight="25.0" prefWidth="206.0" promptText="Password" />
         <TextField fx:id="password2" layoutX="304.0" layoutY="197.0" prefHeight="25.0" prefWidth="206.0" promptText="Password" />
         <Label layoutX="230.0" layoutY="58.0" prefHeight="38.0" prefWidth="149.0" text="Registrazione">
            <font>
               <Font size="25.0" />
            </font>
         </Label>
         <Button fx:id="setUserBtn" layoutX="83.0" layoutY="239.0" mnemonicParsing="false" onAction="#setUser" prefHeight="25.0" prefWidth="77.0" text="Registrati" />
         <Label fx:id="lblError" layoutX="201.0" layoutY="242.0" prefHeight="20.0" prefWidth="206.0" textAlignment="CENTER" textFill="#ff0202">
            <font>
               <Font size="14.0" />
            </font>
         </Label>
         <Button fx:id="BtnSgnIn" layoutX="458.0" layoutY="240.0" mnemonicParsing="false" onAction="#goToSignIn" text="Accedi" />
      </children>
   </AnchorPane>
</Scene>
